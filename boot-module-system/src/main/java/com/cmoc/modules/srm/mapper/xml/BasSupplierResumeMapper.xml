<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cmoc.modules.srm.mapper.BasSupplierResumeMapper">
    <select id="selectByMainId" parameterType="java.lang.String" resultType="com.cmoc.modules.srm.entity.BasSupplierResume">
        SELECT *
        FROM  bas_supplier_resume
        WHERE
            supp_id = #{mainId} 	</select>

    <delete id="deleteByMainId" parameterType="java.lang.String">
        DELETE
        FROM  bas_supplier_resume
        WHERE
            supp_id = #{mainId} 	</delete>
</mapper>